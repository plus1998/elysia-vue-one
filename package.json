{
  "name": "elysia-vue-one",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:app": "cd packages/app && bun run --preload ./src/loader.ts --watch ./src/index.ts",
    "dev:web": "cd packages/web && bun dev",
    "build:app": "cd packages/app && bun build --compile --minify-whitespace --minify-syntax --target bun --outfile build/app src/index.ts",
    "build:web": "cd packages/web && vite build --outDir ../../build/dist",
    "dev": "NODE_ENV=development bun run.ts",
    "build": "bun build.ts"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3"
  }
}